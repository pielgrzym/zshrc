#!/usr/bin/env python2
# encoding: utf-8

import sys
import socket


def getMyIP():
    s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
    s.connect(('google.com', 0))
    return s.getsockname()[0]


for line in sys.stdin:
    if "SCAN:" in line:
        detail_string = line.split('SCAN:')[1].strip()
        details = detail_string.split()
        ip = [e.split('=')[1] for e in details if 'SRC=' in e][0]
        dst = [e.split('=')[1] for e in details if 'DST=' in e][0]
        proto = [e.split('=')[1] for e in details if 'PROTO=' in e][0]
        port = [e.split('=')[1] for e in details if 'DPT=' in e][0]
        if ip != getMyIP():
            r = "${alignr}%s     %s     %s\n" % (ip, proto, port)
            sys.stdout.write(r)
